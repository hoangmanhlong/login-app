<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.loginapp.view.fragment.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_homeFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_homeFragment_to_editUserInformationFragment"
            app:destination="@id/editUserInformationFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_homeFragment_to_expandProductsFragment"
            app:destination="@id/expandProductsFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="com.example.loginapp.view.fragment.product_detail.ProductFragment"
        android:label="ProductFragment" >
        <action
            android:id="@+id/action_productFragment_to_checkoutInfoFragment"
            app:destination="@id/checkoutInfoFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_productFragment_to_selectVoucherFragment"
            app:destination="@id/selectVoucherFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/searchProductFragment"
        android:name="com.example.loginapp.view.fragment.search.SearchProductFragment"
        android:label="SearchProductFragment" >
        <action
            android:id="@+id/action_searchProductFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.loginapp.view.fragment.cart.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_cartFragment_to_checkoutInfoFragment"
            app:destination="@id/checkoutInfoFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_cartFragment_to_selectVoucherFragment"
            app:destination="@id/selectVoucherFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/favoriteProductFragment"
        android:name="com.example.loginapp.view.fragment.favorite_product.FavoriteProductFragment"
        android:label="fragment_favorite_product"
        tools:layout="@layout/fragment_favorite_product" >
        <action
            android:id="@+id/action_favoriteProductFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.example.loginapp.view.fragment.user_profile.UserProfileFragment"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" >
        <action
            android:id="@+id/action_userProfileFragment_to_editUserInformationFragment"
            app:destination="@id/editUserInformationFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_userProfileFragment_to_voucherFragment"
            app:destination="@id/voucherFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_userProfileFragment_to_coinsRewardFragment"
            app:destination="@id/coinsRewardFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>

        <action
            android:id="@+id/action_userProfileFragment_to_ordersFragment"
            app:destination="@id/ordersFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_userProfileFragment_to_deliveryAddressFragment"
            app:destination="@id/deliveryAddressFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>

    </fragment>
    <fragment
        android:id="@+id/coinsRewardFragment"
        android:name="com.example.loginapp.view.fragment.coins.CoinsRewardFragment"
        android:label="fragment_coins_reward"
        tools:layout="@layout/fragment_coins_reward" >
        <action
            android:id="@+id/action_coinsRewardFragment_to_voucherFragment"
            app:destination="@id/voucherFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/editUserInformationFragment"
        android:name="com.example.loginapp.view.fragment.edit_user_profile.EditUserInformationFragment"
        android:label="fragment_edit_user_information"
        tools:layout="@layout/fragment_edit_user_information" />
    <fragment
        android:id="@+id/checkoutInfoFragment"
        android:name="com.example.loginapp.view.fragment.checkout.CheckoutInfoFragment"
        android:label="CheckoutInfoFragment" >
        <action
            android:id="@+id/action_checkoutInfoFragment_to_paymentOptionFragment"
            app:destination="@id/paymentOptionFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
        <action
            android:id="@+id/action_checkoutInfoFragment_to_selectDeliveryAddressFragment"
            app:destination="@id/selectDeliveryAddressFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <dialog
        android:id="@+id/modalBottomSheetFragment"
        android:name="com.example.loginapp.view.fragment.bottom_sheet.ModalBottomSheetFragment"
        android:label="ModalBottomSheetFragment" />
    <fragment
        android:id="@+id/paymentOptionFragment"
        android:name="com.example.loginapp.view.fragment.payment_option.PaymentOptionFragment"
        android:label="PaymentOptionFragment" >
        <action
            android:id="@+id/action_paymentOptionFragment_to_orderSuccessFragment"
            app:destination="@id/orderSuccessFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/orderSuccessFragment"
        android:name="com.example.loginapp.view.fragment.checkout.OrderSuccessFragment"
        android:label="OrderSuccessFragment" >
        <action
            android:id="@+id/action_orderSuccessFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/voucherFragment"
        android:name="com.example.loginapp.view.fragment.voucher.VoucherFragment"
        android:label="VoucherFragment" />
    <fragment
        android:id="@+id/deliveryAddressFragment"
        android:name="com.example.loginapp.view.fragment.shipping_address.DeliveryAddressFragment"
        android:label="ShippingAddressFragment" >
        <action
            android:id="@+id/action_deliveryAddressFragment_to_deliveryAddressDetailFragment"
            app:destination="@id/deliveryAddressDetailFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>

    <fragment
        android:id="@+id/ordersFragment"
        android:name="com.example.loginapp.view.fragment.orders.OrdersFragment"
        android:label="OrdersFragment" >
        <action
            android:id="@+id/action_ordersFragment_to_orderDetailFragment"
            app:destination="@id/orderDetailFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
    <fragment
        android:id="@+id/orderDetailFragment"
        android:name="com.example.loginapp.view.fragment.order_detail.OrderDetailFragment"
        android:label="fragment_order_detail"
        tools:layout="@layout/fragment_order_detail" />

    <fragment
        android:id="@+id/selectVoucherFragment"
        android:name="com.example.loginapp.view.fragment.select_voucher_fragment.SelectVoucherFragment"
        android:label="fragment_select_voucher"
        tools:layout="@layout/layout_select_voucher"/>
    <fragment
        android:id="@+id/selectDeliveryAddressFragment"
        android:name="com.example.loginapp.view.fragment.select_delivery_address.SelectDeliveryAddressFragment"
        android:label="SelectDeliveryAddressFragment" />
    <fragment
        android:id="@+id/deliveryAddressDetailFragment"
        android:name="com.example.loginapp.view.fragment.delivery_address_detail.DeliveryAddressDetailFragment"
        android:label="DeliveryAddressDetail" />
    <fragment
        android:id="@+id/expandProductsFragment"
        android:name="com.example.loginapp.view.fragment.expand_products.ExpandProductsFragment"
        android:label="fragment_recommened_products"
        tools:layout="@layout/fragment_expand_products" >
        <action
            android:id="@+id/action_expandProductsFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right"/>
    </fragment>
</navigation>